{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://collaboration-platform-9ngo.onrender.com",
    "VITE_GRAPHQL_URL": "https://collaboration-platform-9ngo.onrender.com/graphql",
    "VITE_WS_URL": "wss://collaboration-platform-9ngo.onrender.com/graphql"
  }
}